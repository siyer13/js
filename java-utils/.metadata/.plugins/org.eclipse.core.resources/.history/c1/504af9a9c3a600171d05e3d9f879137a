package training.collections;

import java.util.HashSet;
import java.util.Set;

public class Sets {

	public static void main(String[] args) {
		Set<String> fruit_set = new HashSet<String>();
		// set will not allow any duplicates 
		fruit_set.add("apple");
		fruit_set.add("mango");
		fruit_set.add("apple");
		fruit_set.add("banana");

	}

}
